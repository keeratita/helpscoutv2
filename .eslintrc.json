{
  "extends": ["airbnb", "prettier"],
  "env": {
    "node": true,
    "browser": true,
    "mocha": true
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "strict": 0,
    "no-console": 0,
    "new-cap": 0,
    "no-param-reassign": 0,
    "camelcase": 0,
    "no-dynamic-require": 0,
    "no-plusplus": 0,
    "import/no-dynamic-require": 0,
    "consistent-return": 0,
    "no-unused-vars": 0,
    "import/order": 0,
    "arrow-body-style": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0
  }
}